{
  "name": "aws-cdk-template",
  "private": true,
  "license": "UNLICENSED",
  "bin": {
    "aws-cdk-template": "bin/aws-cdk-template.js"
  },
  "scripts": {
    "bootstrap": "cdk bootstrap --profile default",
    "deploy": "cdk deploy --profile default -c env=dev",
    "destroy": "cdk destroy --profile default -c env=dev",
    "diff": "cdk diff --profile default -c env=dev",
    "synth": "cdk synth --profile default -c env=dev",
    "deploy:prod": "cdk deploy --profile default -c env=prod",
    "destroy:prod": "cdk destroy --profile default -c env=prod",
    "diff:prod": "cdk diff --profile default -c env=prod",
    "synth:prod": "cdk synth --profile default -c env=prod"
  },
  "dependencies": {
    "aws-cdk-lib": "2.44.0",
    "constructs": "^10.1.121",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.2",
    "@types/node": "18.8.1",
    "@types/prettier": "2.7.1",
    "@types/source-map-support": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "aws-cdk": "2.44.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4"
  }
}
